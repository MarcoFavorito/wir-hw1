<script language="javascript">
function scroller_Left(seed)
{
        var m1  = "Diploma d'Ingegneria Informatica ed Automatica...";
        var m2  = "";
        
        var msg=m1+m2;
        var out = " ";
        var c   = 1;

        if (seed > 100) {
                seed--;
                var cmd="scroller_Left(" + seed + ")";
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 100 && seed > 0) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
                }
                out+=msg;
                seed--;
                var cmd="scroller_Left(" + seed + ")";
                    window.status=out;
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="scroller_Left(" + seed + ")";
                        window.status=out;
                        timerTwo=window.setTimeout(cmd,100);
                }
                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("scroller_Left(100)",75);
                }
        }
}
</script>


