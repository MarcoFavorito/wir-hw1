 <H1>Solution (cont.nd)</H1> 
 <P><UL>
<H2>3. SELECT S.sname
</H2>
<BR></UL><UL>
<H2>	FROM  Suppliers S
</H2>
</UL><UL>
<H2>	WHERE  NOT EXISTS ((SELECT * FROM Parts P 
</H2>
</UL><UL>
<H2>				     WHERE P.color = ‘red’)
</H2>
</UL><UL>
<H2>				      EXCEPT
</H2>
</UL><UL>
<H2>				    (SELECT C.pid
</H2>
</UL><UL>
<H2>				     FROM Catalog C, Parts P
</H2>
</UL><UL>
<H2>				     WHERE C.sid = S.sid AND
</H2>
</UL><UL>
<H2>					      C.pid = P.pid AND P.color = ‘red’))
</H2>
</UL><UL>
<H2>4. SELECT P.pname
</H2>
</UL><UL>
<H2>	FROM  Parts P, Catalog C, Suppliers S
</H2>
</UL><UL>
<H2>	WHERE  P.pid = C.pid AND C.sid = S.sid
</H2>
</UL><UL>
<H2>		AND S.sname = ‘Acme’
</H2>
</UL><UL>
<H2>		AND NOT EXISTS (SELECT * FROM Catalog C1, Suppliers S1
</H2>
</UL><UL>
<H2>				WHERE P.pid = C1.pid AND C1.sid = S1.sid 					AND S1.sname &#060&#062 ‘Acme’</H2>
</UL></P>
